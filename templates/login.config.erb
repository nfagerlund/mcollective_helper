MCollectivePropertiesLogin {
    org.apache.activemq.jaas.PropertiesLoginModule required
        debug=false
        org.apache.activemq.jaas.properties.user="mcollective-password-users.properties"
        org.apache.activemq.jaas.properties.group="mcollective-groups.properties";
};
MCollectiveCertLogin {
    org.apache.activemq.jaas.TextFileCertificateLoginModule required
        debug=false
        org.apache.activemq.jaas.textfiledn.user="mcollective-certificate-users.properties"
        org.apache.activemq.jaas.textfiledn.group="mcollective-groups.properties";
};
